{
    "name": "@fathy/blog",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "run-s build:gen dev:serve",
        "dev:serve": "webpack serve",
        "build": "npm-run-all --serial build:gen --parallel build:generator build:web --serial build:pages",
        "build:gen": "run-p build:gen:*",
        "build:gen:assets": "scripts/gen-assets.mjs",
        "build:gen:posts": "scripts/gen-posts.mjs",
        "build:generator": "webpack --env prod --env generator",
        "build:web": "webpack --env prod",
        "build:pages": "node build/bundle/generator/prod/main",
        "deploy": "wrangler pages publish build/bundle/web/prod --commit-dirty=true"
    },
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/server": "^11.10.0",
        "@emotion/styled": "^11.10.5",
        "@fontsource/plus-jakarta-sans": "^4.5.10",
        "@fontsource/roboto": "^4.5.8",
        "@mdx-js/react": "^2.1.5",
        "@mui/icons-material": "^5.10.9",
        "@mui/material": "^5.10.11",
        "history": "^5.3.0",
        "prism-themes": "^1.9.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.4.2",
        "react-router-dom": "^6.4.2",
        "tss-react": "^4.4.1"
    },
    "devDependencies": {
        "@babel/core": "^7.19.6",
        "@jsdevtools/rehype-toc": "^3.0.2",
        "@mapbox/rehype-prism": "^0.8.0",
        "@mdx-js/loader": "^2.1.5",
        "@prettier/plugin-xml": "^2.2.0",
        "@svgr/webpack": "^6.5.1",
        "@types/react": "^18.0.24",
        "@types/react-dom": "^18.0.8",
        "copy-webpack-plugin": "^11.0.0",
        "cross-fetch": "^3.1.5",
        "css-loader": "^6.7.1",
        "cssnano": "^5.1.14",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "html-minimizer-webpack-plugin": "^4.3.0",
        "html-webpack-plugin": "^5.5.0",
        "isomorphic-style-loader": "^5.3.2",
        "npm-run-all": "^4.1.5",
        "path-browserify": "^1.0.1",
        "postcss": "^8.4.18",
        "postcss-loader": "^7.0.1",
        "postcss-preset-env": "^7.8.2",
        "prettier": "^2.7.1",
        "rehype-slug": "^5.0.1",
        "remark": "^14.0.2",
        "remark-gfm": "^3.0.1",
        "remark-mdx-images": "^2.0.0",
        "source-map-explorer": "^2.5.3",
        "svgo-loader": "^3.0.3",
        "terser-webpack-plugin": "^5.3.6",
        "ts-loader": "^9.4.1",
        "typescript": "^4.8.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1",
        "wrangler": "^2.1.13"
    }
}
